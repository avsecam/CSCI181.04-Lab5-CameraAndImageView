<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.avsecam.cameraimageview">

    <application
        android:name="com.avsecam.cameraimageview.RealmApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Realm"
        tools:targetApi="31">
        <activity
            android:name="com.avsecam.cameraimageview.EditActivity_"
            android:exported="false" />
        <activity
            android:name="com.avsecam.cameraimageview.AdminActivity_"
            android:exported="false" />
        <activity
            android:name="com.avsecam.cameraimageview.LandingActivity_"
            android:exported="false" />
        <activity
            android:name="com.avsecam.cameraimageview.RegisterActivity_"
            android:exported="false" />
        <activity
            android:name="com.avsecam.cameraimageview.LoginActivity_"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>